{
  "name": "@structurify/sdk",
  "version": "1.0.0",
  "description": "Official Node.js SDK for Structurify document extraction API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "structurify",
    "document",
    "extraction",
    "ai",
    "pdf",
    "ocr",
    "api",
    "sdk"
  ],
  "author": {
    "name": "REDSCVRY TECHNOLOGY PRIVATE LIMITED",
    "email": "support@structurify.ai",
    "url": "https://dscvryai.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DscvryAI/structurify-sdk.git",
    "directory": "nodejs"
  },
  "homepage": "https://structurify.ai",
  "bugs": {
    "url": "https://github.com/DscvryAI/structurify-sdk/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
